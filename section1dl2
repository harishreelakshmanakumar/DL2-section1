        "# Prepare dataset for model fine-tuning\n",
        "import pandas as pd\n",
        "data = {\n",
        "    'url': ['url1', 'url2', 'url3'],\n",
        "    'summary': ['summary1', 'summary2', 'summary3']\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "print(df)\n",
        "\n",
        "articles = df['url'].tolist()\n",
        "summaries = df['summary'].tolist()\n",
        "train_data = []\n",
        "for summary, article in zip(summaries, articles):\n",
        "    train_data.append(f\"Summary: {summary}\\n\\nArticle: {article}\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "GJonS-Ey-wwl",
        "outputId": "504c839f-2ba0-4831-f49e-bd5911505921"
      },
