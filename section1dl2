    {
      "cell_type": "code",
      "source": [
        "from google.colab import drive\n",
        "import pandas as pd\n",
        "data=pd.read_csv('/content/drive/MyDrive/deeplearning 2 dataset/medium_articles.csv')\n",
        "import re\n",
        "import numpy as np\n",
        "from sklearn.metrics.pairwise import cosine_similarity\n",
        "\n",
        "# Step 1: Sentence Tokenization\n",
        "def sentence_tokenize(text):\n",
        "    return sent_tokenize(text)\n",
        "\n",
        "# Step 2: Text Preprocessing\n",
        "def preprocess_sentence(sentence):\n",
        "    sentence = re.sub(r'[^\\w\\s]', '', sentence.lower())\n",
        "    return sentence\n",
        "\n",
        "# Step 3: Remove Stopwords\n",
        "from nltk.corpus import stopwords\n",
        "nltk.download('stopwords')\n",
        "stop_words = set(stopwords.words('english'))\n",
        "\n",
        "def remove_stopwords(sentence):\n",
        "    return ' '.join([word for word in sentence.split() if word not in stop_words])\n",
        "\n",
        "# Step 4: Generate Word Embeddings using Word2Vec\n",
        "def get_word_embeddings(sentences):\n",
        "    word2vec_model = Word2Vec(sentences, vector_size=100, min_count=1, window=5)\n",
        "    return word2vec_model\n",
        "\n",
        "# Step 5: Create Sentence Embeddings\n",
        "def get_sentence_embeddings(sentences, word2vec_model):\n",
        "    sentence_vectors = []\n",
        "    for sentence in sentences:\n",
        "        words = [word for word in sentence.split() if word in word2vec_model.wv]\n",
        "        if len(words) > 0:\n",
        "            vector = np.mean([word2vec_model.wv[word] for word in words], axis=0)\n",
        "        else:\n",
        "            vector = np.zeros((100,))\n",
        "        sentence_vectors.append(vector)\n",
        "    return sentence_vectors\n",
        "\n",
        "# Step 6: Calculate Similarity Matrix\n",
        "def build_similarity_matrix(sentences, sentence_vectors):\n",
        "    similarity_matrix = cosine_similarity(sentence_vectors)\n",
        "    return similarity_matrix\n",
        "\n",
        "# Step 7: Apply PageRank Algorithm\n",
        "def rank_sentences(similarity_matrix):\n",
        "    nx_graph = nx.from_numpy_array(similarity_matrix)\n",
        "    scores = nx.pagerank(nx_graph)\n",
        "    ranked_sentences = sorted(((scores[i], s) for i, s in enumerate(sentences)), reverse=True)\n",
        "    return ranked_sentences\n",
        "\n",
        "# Main function to generate the summary\n",
        "def generate_summary(text, top_n=3):\n",
        "    sentences = sentence_tokenize(text)\n",
        "    preprocessed_sentences = [remove_stopwords(preprocess_sentence(sent)) for sent in sentences]\n",
        "\n",
        "    # Word embeddings\n",
        "    word2vec_model = get_word_embeddings([s.split() for s in preprocessed_sentences])\n",
        "\n",
        "    # Sentence embeddings\n",
        "    sentence_vectors = get_sentence_embeddings(preprocessed_sentences, word2vec_model)\n",
        "\n",
        "    # Similarity Matrix\n",
        "    similarity_matrix = build_similarity_matrix(sentences, sentence_vectors)\n",
        "\n",
        "    # Rank Sentences\n",
        "    ranked_sentences = rank_sentences(similarity_matrix)\n",
        "\n",
        "    # Extract the top ranked sentences\n",
        "    summary = ' '.join([sentences[i] for i in range(top_n)])\n",
        "    return summary\n",
        "\n",
        "# Sample blog content\n",
        "if 'url' in df.columns:\n",
        "    # Function to generate summary (you can replace this with a real model function)\n",
        "    def generate_summary(blog_content):\n",
        "        # Example of simple summarization logic (replace this with your summarization model)\n",
        "        summary = blog_content[:50] + '...'  # Truncate to first 50 characters as a placeholder\n",
        "        return summary\n",
        "\n",
        "    # Extract first blog content\n",
        "    sample_blog = df['url'].iloc[0]\n",
        "\n",
        "    # Generate and print summary\n",
        "    summary = generate_summary(sample_blog)\n",
        "    print(summary)\n",
        "else:\n",
        "    print(\"Error: 'content' column not found in the DataFrame.\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "S4Ka4fIQ04r_",
        "outputId": "db77d7be-ee13-4860-9591-681242366a47"
      },
      "execution_count": 10,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "https://medium.com/invisible-illness/mental-note-v...\n"
          ]
        },
        {
          "output_type": "stream",
          "name": "stderr",
          "text": [
            "[nltk_data] Downloading package stopwords to /root/nltk_data...\n",
            "[nltk_data]   Package stopwords is already up-to-date!\n"
          ]
