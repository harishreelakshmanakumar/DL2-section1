      "cell_type": "code",
      "source": [
        "# Add a new column with the summaries\n",
        "\n",
        "df['summary'] = df['url'].apply(lambda x: generate_summary(x))\n",
        "\n",
        "\n",
        "# Save the dataset with summaries\n",
        "df[['title', 'summary', 'url']].to_csv('medium_articles.csv', index=False)\n"
