        "from transformers import GPT2Tokenizer, GPT2LMHeadModel\n",
        "\n",
        "# Load the tokenizer and model for text generation\n",
        "tokenizer = GPT2Tokenizer.from_pretrained('gpt2')\n",
        "model = GPT2LMHeadModel.from_pretrained('gpt2')\n",
        "\n",
        "# Input a summary and generate a blog\n",
        "input_summary = \"A summary of the blog here\"\n",
        "\n",
        "# Encode the input\n",
        "inputs = tokenizer.encode(input_summary, return_tensors='pt')\n",
        "\n",
        "# Generate the output\n",
        "outputs = model.generate(inputs, max_length=512, num_return_sequences=1)\n",
        "\n",
        "# Decode the output to text\n",
        "generated_blog = tokenizer.decode(outputs[0], skip_special_tokens=True)\n",
        "print(generated_blog)\n"
